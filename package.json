{
  "name": "chatbox-extension",
  "displayName": "Chatbox Extension",
  "description": "A simple chatbox extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onCommand:chatbox.start"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatbox.start",
        "title": "Start Chatbox"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No compilation needed for JavaScript'"
  }
}
